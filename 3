#include <iostream>
#include <cmath>
using namespace std;
int main() {
    long long N;
    cin >> N;
    long long count = 0;
    for (long long i = 1; i * i <= N; i++) {
        if (N % i == 0) {
            count++; // i là ước
            if (i != N / i) {
                count++; //
            }
        }
    }
    cout << count << endl;
    return 0;
}
